    "build:types": "tsc",
    "lint": "eslint --ext .js src"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "magic-string": "^0.30.21",
    "redux": "^5.0.1",
    "redux-saga": "^1.4.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^4.1.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "webpack": "^5.104.1"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 6.0.0"
